{
	"name": "stream-keepr",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
	"scripts": {
		"clean": "rm -rf node_modules pnpm-lock.yaml && pnpm --filter '**' clean",
		"nuxt:dev": "pnpm --filter web dev",
		"nuxt:build": "pnpm --filter web build",
		"nuxt:deploy": "pnpm --filter web deploy",
		"workers:dev": "pnpm --filter workers dev",
		"workers:push": "pnpm --filter workers push",
		"workers:types": "pnpm --filter workers types"

	},
	"devDependencies": {
		"@stream-keepr/eslint": "workspace:*",
		"eslint": "catalog:",
		"typescript": "catalog:"
	}
}
